{
  "ConnectionStrings": {
    "StoreDBConn": "Server=localhost;Database=storeDB;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=False;Encrypt=False"
  },
"Serilog": {
  "MinimumLevel": {
    "Default": "Information",
    "Override": {
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WriteTo":
  [{
    "Name": "File",
    "Args": {
      "path": "./logs/log-.txt",
      "rollingInterval": "Day"
    }
    
  }
    ]
},
  "AllowedHosts": "*"
}
